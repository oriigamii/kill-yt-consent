{
  "manifest_version": 2,
  "name": "Kill YT Consent",
  "description": "Prevent Youtube to display consent bump",
  "version": "1.0",
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "http://www.youtube.com/",
        "https://www.youtube.com/"
      ],
      "js": [
        "background.js"
      ],
      "run_at": "document_end"
    }
  ],
  "permissions": [
    "tabs",
    "activeTab",
    "contextMenus",
    "notifications",
    "http://www.youtube.com/",
    "https://www.youtube.com/",
    "webNavigation",
    "storage"
  ],
  "options_page": "menu-options.html",
  "options_ui": {
    "chrome_style": true,
    "page": "menu-options.html",
    "open_in_tab": false
  },
  "browser_action": {
    "default_popup": "menu-options.html"
  }
}